// DOBRE
/* OUTPUT:
    1  neparne
    gaee
    2  parne
    gaee
    3  neparne
    gaee
    4  parne
    gaee
    5  neparne
    gaee
*/
func gae() -> Int {
    var a = 1
    var b = 5
    while a <= b {
        if a % 2 == 0 {
            write(a, " parne")
        } else {
            write(a, " neparne")
        }
        write("gaee")
        a = a + 1
    }
    return 0
}

gae()