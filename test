.IFJcode23
JUMP MAIN

LABEL readInt
CREATEFRAME
PUSHFRAME
DEFVAR LF@retval
MOVE LF@retval nil@nil
READ LF@retval int
POPFRAME
RETURN
LABEL readString
CREATEFRAME
PUSHFRAME
DEFVAR LF@retval
MOVE LF@retval nil@nil
READ LF@retval string
POPFRAME
RETURN
LABEL print
PUSHFRAME
DEFVAR LF@writeval
MOVE LF@writeval LF@%1
WRITE LF@writeval
POPFRAME
RETURN

LABEL MAIN
DEFVAR GF@exp
DEFVAR GF@meow
PUSHS int@0
POPS GF@exp
MOVE GF@meow GF@exp
CREATEFRAME
DEFVAR TF@retval
CALL readInt
PUSHS TF@retval
POPS GF@meow
CREATEFRAME
DEFVAR TF@%1
MOVE TF@%1 GF@meow
CALL print
CREATEFRAME
DEFVAR TF@%1
MOVE TF@%1 string@\010
CALL print
DEFVAR GF@hey
CREATEFRAME
DEFVAR TF@retval
CALL readString
PUSHS TF@retval
POPS GF@exp
MOVE GF@hey GF@exp
CREATEFRAME
DEFVAR TF@%1
MOVE TF@%1 GF@hey
CALL print
EXIT int@0
