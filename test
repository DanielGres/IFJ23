.IFJcode23
JUMP MAIN

LABEL readInt
CREATEFRAME
PUSHFRAME
DEFVAR LF@retval
MOVE LF@retval nil@nil
READ LF@retval int
POPFRAME
RETURN
LABEL readString
CREATEFRAME
PUSHFRAME
DEFVAR LF@retval
MOVE LF@retval nil@nil
READ LF@retval string
POPFRAME
RETURN
LABEL print
PUSHFRAME
DEFVAR LF@writeval
MOVE LF@writeval LF@%1
WRITE LF@writeval
POPFRAME
RETURN

LABEL MAIN
DEFVAR GF@exp
DEFVAR GF@a
PUSHS int@5
POPS GF@exp
MOVE GF@a GF@exp
LABEL WHILE1
CREATEFRAME
DEFVAR TF@%1
MOVE TF@%1 string@a\032is\032
CALL print
CREATEFRAME
DEFVAR TF@%1
MOVE TF@%1 GF@a
CALL print
CREATEFRAME
DEFVAR TF@%1
MOVE TF@%1 string@\010
CALL print
PUSHS int@1
PUSHS GF@a
ADDS
POPS GF@a
PUSHS int@20
PUSHS GF@a
EQS
POPS GF@exp
JUMPIFEQ WHILE1 GF@exp bool@false
EXIT int@0
