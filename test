.IFJcode23
JUMP MAIN

LABEL readInt
CREATEFRAME
PUSHFRAME
DEFVAR LF@retval
MOVE LF@retval nil@nil
READ LF@retval int
POPFRAME
RETURN
LABEL readString
CREATEFRAME
PUSHFRAME
DEFVAR LF@retval
MOVE LF@retval nil@nil
READ LF@retval string
POPFRAME
RETURN
LABEL print
PUSHFRAME
DEFVAR LF@writeval
MOVE LF@writeval LF@%1
WRITE LF@writeval
POPFRAME
RETURN

LABEL MAIN
DEFVAR GF@exp
DEFVAR GF@a
PUSHS int@4
POPS GF@exp
MOVE GF@a GF@exp
DEFVAR GF@b
PUSHS float@2.0
POPS GF@exp
MOVE GF@b GF@exp
DEFVAR GF@c
PUSHS GF@b
PUSHS GF@a
DIVS
POPS GF@exp
MOVE GF@c GF@exp
EXIT int@0
